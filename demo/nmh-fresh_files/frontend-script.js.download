function builder_stars(jEle) {

    var setup = jEle.attr('builder-setup');
    if (setup && setup.length > 0) {
        return true;
    }
    var count = jEle.attr('builder-stars-count');

    if (isNaN(count)) {
        count = '0';
    }

    i = 0;
    var stars = "";
    while (i < count) {
        stars += '<div class="builder-stars-icon builder-stars-empty"><i class="fas fa-star" aria-hidden="true"></i></div>';
        i++;
    }

    jEle.empty();
    jEle.append(stars);
    var starsval = jEle.attr('builder-stars-value');

    if (isNaN(starsval)) {
        starsval = count;
    }

    starsval = starsval.split('.');
    var fullstars = starsval[0];
    var value = starsval[1];
    var halfstar = parseInt(fullstars) + 1;
    var emptystars = parseInt(fullstars) + 2;
    jEle.children('.builder-stars-icon').attr("class", "builder-stars-icon");
    jEle.children('.builder-stars-icon:nth-child(-n+' + fullstars + ')').addClass('builder-stars-full');
    if (value != undefined) {
        jEle.children('.builder-stars-icon:nth-child(' + halfstar + ')').addClass('builder-stars-' + value);
    } else {
        jEle.children('.builder-stars-icon:nth-child(' + halfstar + ')').addClass('builder-stars-empty');
    }
    jEle.children('.builder-stars-icon:nth-child(n+' + emptystars + ')').addClass('builder-stars-empty');
    jEle.attr('builder-setup', 1);
}

// Grid Gallery pagination Off On function
function builder_pl_grid_paginate(gridCont, pagination, pageValue, gridValue) {
    gridCont.hide();
    pagination.removeClass('active');
    pagination.eq(pageValue).addClass('active');
    gridCont.eq(gridValue).show();
