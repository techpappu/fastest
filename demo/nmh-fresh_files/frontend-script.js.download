function builder_get_tab_ele(temp_tabCont) {

    if (!builder_empty(temp_tabCont.children('.builder-ele-wrap').length)) {
        return temp_tabCont.children('.builder-ele-wrap').children('.builder-tab');
    } else {
        return temp_tabCont.children('.builder-tab');
    }
}

function builder_tab_show(el, pl_id) {

    var parent_id = jQuery(el).closest('.builder-tabs').attr('builder-id');

    var temp_tabCont = builder_query('[builder-id=' + pl_id + ']').closest('.builder-tabcontainer');
    builder_get_tab_ele(temp_tabCont).hide();

    builder_query('[builder-id=' + pl_id + ']').show();

    jQuery(el).parent().find('.builder-tablinks').each(function () {
        jQuery(this).removeClass('active');
    });

    jQuery(el).addClass("active");
}

var builder_tab_timers = {};

function builder_pl_tabs(jEle) {

    var default_active = '';
    var jEle_id = jEle.attr('builder-id');

    var tabCont = jEle.children('.builder-tabcontainer');
    var children = builder_get_tab_ele(tabCont);

    // Loop thru
    children.each(function () {
        var tEle = jQuery(this);
        var pl_id = tEle.attr('builder-id');

        var title = tEle.attr('builder-tab-title') || 'Tab';
        var func = "builder_tab_show(this, '" + pl_id + "')";

        var icon = '';
        if (tEle.attr('builder-tab-icon')) {
            icon = tEle.attr('builder-tab-icon');
        }

        // Set the default tab
        if (tEle.attr('builder-default_active') && !builder_empty(pl_id)) {
            default_active = pl_id;
        }

        jEle.children('.builder-tabs-holder').append('<span tab-id="' + pl_id + '" class="builder-tablinks" onclick="' + func + '"> <i class="' + icon + '"></i> <span>' + title + '</span></span>');
    });

    // Set the default tab
    if (default_active.length > 0) {
        builder_tab_show(jEle.find('[tab-id=' + default_active + ']'), default_active);
        // Set the first tab as active
    } else {
        var first_tab = jEle.find('[tab-id]').first();
        builder_tab_show(first_tab, first_tab.attr('tab-id'));
    }

    try {
        clearInterval(builder_tab_timers[jEle_id]);
    } catch (e) { };

    var rotate = parseInt(jEle.attr('builder-tabs-rotate'));

    // Are we to rotate
    if (rotate > 0) {

        var i = 0;
        builder_tab_timers[jEle_id] = setInterval(function () {

            if (i >= children.length) {
                i = 0;
            }

            var tabCont = jEle.children('.builder-tabcontainer');
            var tmp_pl_ele = builder_get_tab_ele(tabCont)[i];

            var tmp_btn_ele = jEle.find('.builder-tablinks')[i]
            var tmp_pl_id = jQuery(tmp_pl_ele).attr('builder-id');

            jEle.find('.builder-tablinks').each(function () {
                jQuery(this).removeClass('active');
            });

            jQuery(tmp_btn_ele).addClass("active");
            builder_tab_show(tmp_btn_ele, tmp_pl_id);

            i++;

        }, rotate);
    }

}

// Setup the Accordion
function builder_pl_accordion(jEle) {

    var holder = jEle.find('.builder-accordion-holder');
    var accHolder = jEle.find('.builder-accordion_item');
    var scrolltop = false;

    if (accHolder.length < 1) {
        return false;
    }

    var icon = holder.attr('data-icon');
    var active_icon = holder.attr('data-active_icon');

    accHolder.find('.builder-accordion-tabs span i').attr('class', icon);
    var currentActiveTab = jEle.find('.builder-accordion_item.active').first();

    // Any URL HASH ?
    var hash = location.hash.slice(1);
    if (!builder_empty(hash)) {
        var scrollTab = jEle.find('#' + hash);

        if (!builder_empty(scrollTab) && scrollTab.length > 0) {
            currentActiveTab = scrollTab.closest('.builder-accordion_item');
        }
    }

    holder.unbind('click');
    holder.on('click', '.builder-accordion-tabs', function () {

        var currentTab = jQuery(this).closest('.builder-accordion_item');

        if (currentTab.hasClass('active') && currentTab.children('.builder-accordion-panel').is(':visible')) {
            currentTab.removeClass('active').children('.builder-accordion-panel').slideUp('slow');
            currentTab.find('.builder-accordion-tabs span i').attr('class', icon);
            return true;
        }

        accHolder.find('.builder-accordion-tabs span i').attr('class', icon);
        accHolder.removeClass('active').filter(function (index) {
            return accHolder[index] != currentTab[0];
        }).children('.builder-accordion-panel').slideUp('slow');

        currentTab.addClass('active').children('.builder-accordion-panel').slideDown('slow');
        currentTab.find('.builder-accordion-tabs span i').attr('class', active_icon);

    });

    // To prevent default active click in gutenberg
    var setup = jEle.attr('builder-setup');

    // Already setup ?
    if (setup && setup.length > 0) {
        return;
    }

    jEle.attr('builder-setup', 1);

    // If active first tab from all active tabs
    currentActiveTab.removeClass('active');
    currentActiveTab.find('.builder-accordion-tabs').click();
}

// Setup the Collapse
function builder_pl_collapse(jEle) {

    var holder = jEle.find('.builder-collapse-holder');
    var tabs = jEle.find('.builder-accordion_item');

    if (tabs.length < 1) {
        return false;
    }

    var setup = tabs.attr('builder-setup');
    var icon = holder.attr('data-icon');
    var active_icon = holder.attr('data-active_icon');

    // Any URL HASH ?
    var hash = location.hash.slice(1);
    if (!builder_empty(hash)) {
        var scrollTab = jEle.find('#' + hash);

        if (!builder_empty(scrollTab) && scrollTab.length > 0) {
            scrollTab.closest('.builder-accordion_item').addClass('active');
        }
    }

    var activeTabs = jEle.find('.builder-accordion_item.active');

    tabs.find('.builder-accordion-tabs span i').attr('class', icon);
    jQuery(activeTabs).addClass('active').children('.builder-accordion-panel').slideDown('slow');
    jQuery(activeTabs).find('.builder-accordion-tabs span i').attr('class', active_icon);

    // Already setup ?
    if (setup && setup.length > 0) {
        tabs.find('.builder-accordion-tabs').unbind('click');
    }

    tabs.find('.builder-accordion-tabs').click(function () {

        var currentTab = jQuery(this).closest('.builder-accordion_item');

        if (currentTab.hasClass('active')) {
            currentTab.removeClass('active').children('.builder-accordion-panel').slideUp('slow');
            currentTab.find('.builder-accordion-tabs span i').attr('class', icon);
            return true;
        }

        currentTab.addClass('active').children('.builder-accordion-panel').slideDown('slow');
        currentTab.find('.builder-accordion-tabs span i').attr('class', active_icon);

    });

    // Set that we have setup everything
    tabs.attr('builder-setup', 1);

}

// Counter
function builder_counter() {

    builder_query('.builder-counter-content').each(function () {

        var jEle = jQuery(this);

        if (builder_isVisible(jEle)) {

            var setup = jEle.attr('builder-setup');

            // Already setup ?
            if (setup && setup.length > 0) {
                return true;
            }

            var options = {};
            options['duration'] = jEle.children('.builder-counter-display').attr('builder-counter-animation-duration');
            options['delimiter'] = jEle.children('.builder-counter-display').attr('builder-counter-seperator-type');
            options['toValue'] = jEle.children('.builder-counter-display').attr('builder-counter-last-value');
            jEle.children('.builder-counter-display').numerator(options);

            // Set that we have setup everything
            jEle.attr('builder-setup', 1);

        }
    });
}

function builder_progress() {
    builder_query('.builder-progress-container').each(function () {
        var jEle = jQuery(this);

        if (builder_isVisible(jEle)) {

            var setup = jEle.attr('builder-setup');
            if (setup && setup.length > 0) {
                return true;
            }

            var progress_width = jEle.children('.builder-progress-bar').attr('builder-progress-width');
            if (progress_width == undefined) {
                progress_width = "1";
            }

            var width = 0;
            var interval;

            var progress = function () {
                if (width >= progress_width) {
                    clearInterval(interval);
                } else {
                    width++;
                    jEle.children('.builder-progress-bar').css('width', width + '%');
                    jEle.find('.builder-progress-percent').text(width * 1 + '%');
                }
            }
            interval = setInterval(progress, 30);
            jEle.attr('builder-setup', 1);

        }
    });
}

