// Contact Form Submit handler - Premium
function builder_contact_submit(jEle, e) {
    e.preventDefault();

    // Checking for required checkboxes.
    for (var checkbox_div of jQuery(jEle).find('.builder-contact-checkbox')) {
        checkbox_div = jQuery(checkbox_div);
        if (checkbox_div.attr('required') == 'required') {
            if (builder_empty(checkbox_div.find('input:checked').length)) {
                alert('Kindly select the required checkbox');
                return;
            }
        }
    }

    // Trigger an action
    jQuery(document).trigger('builder_contact_submit', e, jEle);

    // Disabling submit button with loading animation.
    jQuery(jEle).find('.builder-contact-submit-btn').prop('disabled', true);
    jQuery(jEle).find('.builder-contact-submit-btn .fa-spin').show('0.6');

    //var fdata = jQuery(jEle).closest('form').serialize();
    var redirect = jQuery(jEle).find('input[name="cfa-redirect"]');
    var formData = new FormData(jQuery(jEle)[0]);
    var par = jQuery(jEle).parent();

    // Append the nonce
    formData.append('_token', builder_ajax_nonce);

    // Hide any message
    par.find(".builder-message-box").hide();

    // Message pos to use ?
    var msg_pos = 'top';
    if (par.parent().hasClass('builder-message-box-bottom')) {
        msg_pos = 'bottom';
    }

    par.find(".builder-message-box").removeClass('builder-cf-msg-err builder-cf-msg-suc');

    jQuery.ajax({
        url: builder_ajaxurl + 'action=builder_contact_submit',
        type: "POST",
        data: formData,
        processData: false,
        contentType: false,
        cache: false,
        success: function (result) {
            var json = jQuery.parseJSON(result);

            jQuery(jEle).find('.builder-contact-submit-btn').prop('disabled', false);
            jQuery(jEle).find('.builder-contact-submit-btn .fa-spin').hide('0.6');

            if ('success' in json) {
                par.find(".builder-message-" + msg_pos).addClass('builder-cf-msg-suc').html(json['success']).fadeIn();

                if (redirect.length > 0 && !builder_empty(redirect.val())) {
                    window.location.href = redirect.val();
                }
            } else {
                par.find(".builder-message-" + msg_pos).addClass('builder-cf-msg-err').html(json['failed']).fadeIn();
            }
        },
        error: function (result) {
            par.find(".builder-message-" + msg_pos).addClass('builder-cf-msg-err').html(json['failed']).fadeIn();
        }
    });
    jEle.reset();

    jQuery(jEle).find('.builder-recaptcha').each(function () {
        var widgetID = jQuery(this).attr('recaptcha-widget-id');

        if (!builder_empty(window.grecaptcha)) {
            grecaptcha.reset(widgetID);
        }
    });

    return false;
}

// Primary Menu Handler - Premium
function builder_primary_menu(jEle) {

    var container = jEle.find('.builder-wp-menu-container');
    var menu_bar = jEle.find('.builder-primary-menu-bar i');
    var menu_holder = jEle.find('.builder-wp-menu-holder');
    var layout = menu_holder.attr('data-layout');
    var submenu_ind = menu_holder.attr('data-submenu_ind');
    var responsive = menu_holder.attr('data-responsive');
    var drop_breakpoint = menu_holder.attr('data-drop_breakpoint');
    var close = jEle.find('.builder-wp_menu-close');

    var media_mode = builder_get_media_mode();

    if ((drop_breakpoint == 'tablet' && (media_mode == 'tablet' || media_mode == 'mobile')) || (drop_breakpoint == 'mobile' && media_mode == 'mobile')) {

        menu_holder.addClass('builder-wp-menu-dropdown');
        container.addClass('builder-menu-type-dropdown');
        container.removeClass('builder-menu-type-' + layout);
        layout = 'dropdown';

    } else {
        menu_holder.removeClass('builder-wp-menu-dropdown');
        container.removeClass('builder-menu-type-dropdown');
        container.addClass('builder-menu-type-' + layout);
    }

    // Set mega menu width
    // Wait for all other animations to finish
    setTimeout(function () {

        container.find('.builder-mega-menu-item, .builder-mega-column-item').each(function () {

            var liEle = jQuery(this),
                lEle = liEle.find('.builder-nav_menu_item').first(),
                megaHolder = lEle.closest('.builder-mega-menu'),
                setClass = 'builder-set-position';

            if (liEle.hasClass('builder-mega-column-item')) {
                megaHolder = liEle.children('.sub-menu');
            }

            if (megaHolder.length < 1) {
                return;
            }

            var Css = {};

            // Remove all css settings
            jQuery(document).unbind('scroll.megaMenu');
            megaHolder.css({ 'width': '', 'left': '', 'max-width': '', 'max-height': '' });

            if (layout == 'dropdown') {
                return;
            }

            // Set active to get position
            megaHolder.addClass(setClass);

            var megaLeft = megaHolder.offset().left,
                megaWidth = lEle.attr('builder-mega-width'),
                wContainer = lEle.closest('.builder-wp-menu-container'),
                megaCustomWidth = lEle.attr('builder-mega-custom-width') || '',
                widthEle;

            // Is vertical menu?
            if (layout == 'vertical') {
                var docWidth = jQuery('body').width();
                var vWidth = docWidth - megaLeft;

                Css['max-width'] = vWidth;
                Css['width'] = vWidth;

                if (megaWidth == 'custom') {
                    Css['width'] = megaCustomWidth;
                }

                megaHolder.css(Css);
                megaHolder.removeClass(setClass);
                return;
            }

            var megaMenuHeight = function (e) {

                if (!builder_empty(e) && megaHolder.is(':visible')) {
                    return;
                }

                var windowHeight = jQuery(window).height();
                var ulBottom = megaHolder.closest('.builder-wp_menu-ul')[0].getBoundingClientRect().bottom;
                megaHolder.css('max-height', windowHeight - ulBottom);
            };

            megaMenuHeight();
            jQuery(document).on('scroll.megaMenu', megaMenuHeight);

            switch (megaWidth) {
                case 'row_container':
                    widthEle = lEle.closest('.builder-row[builder-id]');
                    Css['width'] = widthEle.width();
                    break;
                case 'custom':
                    widthEle = lEle.closest('li');
                    Css['width'] = megaCustomWidth;
                    break;
                default:
                    widthEle = wContainer;
                    Css['width'] = widthEle.width();
            }

            if (widthEle.length > 0) {
                var wLeft = widthEle.offset().left;

                if (wLeft < megaLeft) {
                    Css['left'] = (wLeft) - (megaLeft);
                }

            }

            megaHolder.css(Css);
            var mRect = megaHolder[0].getBoundingClientRect();
            var wRect = wContainer[0].getBoundingClientRect();

            // Set mega menu position
            if (megaWidth != 'custom' || mRect.right < wRect.right) {
                megaHolder.removeClass(setClass);
                return;
            }

            var left = parseInt(megaHolder.css('left'));
            var moveLeft = mRect.right - wRect.right;

            if (mRect.left < moveLeft) {
                moveLeft = moveLeft - (moveLeft - mRect.left);
            }

            left = left - moveLeft;
            megaHolder.css({ 'left': left });
            megaHolder.removeClass(setClass);
        });

    }, 500);

    // Menu toggle
    var toggle_class;
    jQuery(menu_bar).unbind('click');
    jQuery(menu_bar).click(function () {
        jQuery(container).toggleClass('builder-togglt-on');

        toggle_class = jQuery(this).data('icon');
        toggle_class = (builder_empty(toggle_class) ? 'fas fa-bars' : toggle_class);

        if (jQuery(container).hasClass('builder-togglt-on')) {
            jQuery(this).removeClass(toggle_class);
            jQuery(this).addClass('fas fa-times');
        } else {
            jQuery(this).removeClass('fas fa-times');
            jQuery(this).addClass(toggle_class);
        }
    });

    // If has sub-menu the as icon
    var sub_menuEle = jQuery(container).find('.builder-wp_menu-ul li.menu-item-has-children:not(.builder-mega-menu-item), .builder-wp_menu-ul li.builder-mega-menu-item');

    var aEle_sub_menu = sub_menuEle.children('a');
    var afterIconSpan = aEle_sub_menu.children('.after-icon')
    if (aEle_sub_menu.children('.after-icon').length < 1) {
        aEle_sub_menu.append('<span class="after-icon fa fa-' + submenu_ind + '"></span>');
    } else {
        // For gutenberg
        afterIconSpan.attr('class', 'after-icon fa fa-' + submenu_ind);
    }

    // Toggle Sub nav
    var after_icon = jQuery(container).find('.builder-wp_menu-ul li.menu-item-has-children .after-icon, .builder-wp_menu-ul li.builder-mega-menu-item .after-icon');

    after_icon.unbind('click');
    after_icon.click(function (e) {
        e.preventDefault();
        if (window.matchMedia("(max-width: " + builder_settings['tablet_breakpoint'] + "px)").matches || layout != 'horizontal') {
            jQuery(this).closest('li').toggleClass('builder-active-sub-menu');

        } else {
            jQuery(this).closest('li').removeClass('builder-active-sub-menu');
        }
    });

    close.unbind('click');
    close.click(function () {
        jQuery(container).toggleClass('builder-togglt-on');
        jQuery(menu_bar).removeClass('fas fa-times');
        jQuery(menu_bar).addClass(toggle_class);
    });

    // To edit the mega menu in live editor
    jQuery(document).trigger('builder_primary_menu_setup_end', [jEle]);
}

var count_int = {};
// Show countdown render
function builder_countdown(jEle) {

    var expiry_date = jEle.find('.builder-countdown-container').attr('builder-expiry-date');
    var timetype = jEle.find('.builder-countdown-container').attr('builder-time-type');
    var jEle_id = jEle.attr('builder-id');

    if (builder_empty(expiry_date) || expiry_date == "{{date}}") {
        var expiry_date = new Date(new Date().getTime() + 24 * 60 * 60 * 1000);
    }

    var now;
    if (timetype == "server") {
        now = new Date(builder_server_time * 1000).getTime();
    } else {
        now = new Date().getTime();
    }

    var countDownDate = new Date(expiry_date).getTime();
    var distance = countDownDate - now;

    clearInterval(count_int[jEle_id]);
    count_int[jEle_id] = setInterval(function () {

        // Time calculations for days, hours, minutes and seconds
        var days = Math.floor(distance / (1000 * 60 * 60 * 24));
        var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        var seconds = Math.floor((distance % (1000 * 60)) / 1000);

        jEle.find('.builder-days-count').html(days);
        jEle.find('.builder-hours-count').html(hours);
        jEle.find('.builder-minutes-count').html(minutes);
        jEle.find('.builder-seconds-count').html(seconds);

        // If the count down is over, write some text
        if (distance < 0) {
            clearInterval(count_int[jEle_id]);
            jEle.find('.builder-countdown-expired').show();
            jEle.find('.builder-countdown-counter').hide();
        }

        distance = distance - 1000;

    }, 1000);
}

