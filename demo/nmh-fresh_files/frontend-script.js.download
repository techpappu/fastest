// Post infinite scroll handler
function builder_infinite_posts(jEle) {

    var loader = jEle.find('.builder-btn-load');
    var autoScroll = jEle.find('.builder-infinite-scroll-auto').length < 1;

    loader.on('click', function () {

        var bEle = jQuery(this);
        var loaded = jEle.attr('builder-post-data-loading');

        // Is loading?
        if (!builder_empty(loaded)) {
            return;
        }

        jEle.attr('builder-post-data-loading', 1);

        var current = bEle.attr('data-current') || 1;
        var nextPage = parseInt(current) + 1;
        var load_btn = jEle.find('.builder_load_button');

        bEle.hide();
        load_btn.find('.builder-loader-holder').show();

        if (jEle.find('.builder-post-max').attr('data-max') <= 1) {
            load_btn.text(load_btn.data('text'));
            return;
        }

        // Get Data from local variable
        var data = window['builder_local_scripts']['builder_post_' + jEle.attr("builder-id")];

        // Add next page number to load
        if (!builder_empty(data.atts)) {
            data.atts['paged'] = nextPage;
        }

        // Get the Posts
        jQuery.ajax({
            url: builder_ajaxurl + 'action=builder_infinite_posts',
            type: 'POST',
            data: {
                _token: builder_ajax_nonce,
                data: data,
            },
            success: function (result) {

                var json = jQuery.parseJSON(result);
                var content = jQuery(json['posts']).find('.builder-posts-container').html();

                jEle.find('.builder-posts-container').append(content);

                load_btn.find('.builder-loader-holder').hide();

                if (jEle.find('.builder-post-max').attr('data-max') == nextPage) {
                    load_btn.text(load_btn.data('text'));
                } else if (autoScroll) {
                    bEle.show();
                }

                bEle.attr('data-current', nextPage);
            },
            complete: function () {
                jEle.removeAttr('builder-post-data-loading');
            }
        });

    });

    // If already scrolled
    if (builder_isVisible(loader)) {
        loader.click();
    }

    // Auto scroll?
    if (autoScroll) {
        return;
    }

    var win = jQuery(builderGetCurrentWindow());

    win.on('scroll.archive_posts', function () {
        var current = parseInt(loader.attr('data-current')) || 1;
        var total = loader.attr('data-max');

        if (win.scrollTop() + win.height() < jEle.height() || current >= total) {
            return;
        }

        loader.click();
    });

}


