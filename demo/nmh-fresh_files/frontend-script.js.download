var builderAnimTimer = {};
function builder_anim_heading(jEle) {
    var animationDelay = 2500,
        //loading bar effect
        barAnimationDelay = 3800,
        barWaiting = barAnimationDelay - 3000, //3000 is the duration of the transition on the loading bar - set in the scss/css file
        //letters effect
        lettersDelay = 50,
        //type effect
        typeLettersDelay = 150,
        selectionDuration = 500,
        typeAnimationDelay = selectionDuration + 800,
    //clip effect
        revealDuration = 600,
        revealAnimationDelay = 1500;

    var id = jEle.attr('builder-id');

    if (builder_empty(builderAnimTimer[id])) {
        builderAnimTimer[id] = {};
    }

    // Clear the previous animations
    for (var time in builderAnimTimer[id]) {
        clearTimeout(builderAnimTimer[id][time]);
    }

    initHeadline();

    function initHeadline() {

        // Re-set styles
        jEle.find('.builder-aheading-holder').find('span').css('opacity', '');

        //insert <i> element for each letter of a changing word
        singleLetters(jEle.find('.builder-aheading-holder.letters').find('span'));
        //initialise headline animation
        animateHeadline(jEle.find('.builder-aheading-holder'));
    }

    function singleLetters($words) {
        $words.each(function () {
            var word = jQuery(this),
                letters = word.text().split(''),
                selected = word.hasClass('builder-is-visible');
            for (i in letters) {
                if (word.parents('.builder-aheading-rotate2').length > 0) letters[i] = '<b>' + letters[i] + '</b>';
                letters[i] = (selected) ? '<strong class="builder-aheading-in">' + letters[i] + '</strong>' : '<strong>' + letters[i] + '</strong>';
            }
            var newLetters = letters.join('');
            word.html(newLetters).css('opacity', 1);
        });
    }

    function animateHeadline($headlines) {
        var duration = animationDelay;
        $headlines.each(function () {
            var headline = jQuery(this);

            if (headline.hasClass('builder-aheading-loading-bar')) {
                duration = barAnimationDelay;
                setTimeout(function () { headline.find('.builder-words-wrapper').addClass('builder-is-loading') }, barWaiting);
            } else if (headline.hasClass('builder-aheading-clip')) {
                var spanWrapper = headline.find('.builder-words-wrapper'),
                    newWidth = spanWrapper.width() + 10;
                spanWrapper.css('width', newWidth);
            } else if (!headline.hasClass('type')) {
                var words = headline.find('.builder-words-wrapper span'),
                    width = 0;
                words.each(function () {
                    var wordWidth = jQuery(this).width();
                    if (wordWidth > width) width = wordWidth;
                });
                headline.find('.builder-words-wrapper').css('width', width);
            };

            //trigger animation
            setTimeout(function () {
                hideWord(headline.find('.builder-is-visible').eq(0));
            }, duration);
        });
    }

    function hideWord($word) {
        var nextWord = takeNext($word);

        if ($word.parents('.builder-aheading-holder').hasClass('letters')) {
            var bool = ($word.children('strong').length >= nextWord.children('strong').length) ? true : false;
            hideLetter($word.find('strong').eq(0), $word, bool, lettersDelay);
            showLetter(nextWord.find('strong').eq(0), nextWord, bool, lettersDelay);

        } else if ($word.parents('.builder-aheading-holder').hasClass('builder-aheading-clip')) {
            $word.parents('.builder-words-wrapper').animate({ width: '2px' }, revealDuration, function () {
                switchWord($word, nextWord);
                showWord(nextWord);
            });

        } else if ($word.parents('.builder-aheading-holder').hasClass('builder-aheading-loading-bar')) {
            $word.parents('.builder-words-wrapper').removeClass('builder-is-loading');
            switchWord($word, nextWord);

            clearTimeout(builderAnimTimer[id][0]);
            builderAnimTimer[id][0] = setTimeout(function () {
                hideWord(nextWord);
            }, barAnimationDelay);
            clearTimeout(builderAnimTimer[id][1]);
            builderAnimTimer[id][1] = setTimeout(function () {
                $word.parents('.builder-words-wrapper').addClass('builder-is-loading');
            }, barWaiting);

        } else {
            switchWord($word, nextWord);
            clearTimeout(builderAnimTimer[id][2]);
            builderAnimTimer[id][2] = setTimeout(function () {
                hideWord(nextWord);
            }, animationDelay);
        }
    }

    function showWord($word, $duration) {
        if ($word.parents('.builder-aheading-holder').hasClass('builder-aheading-clip')) {
            $word.parents('.builder-words-wrapper').animate({ 'width': $word.width() + 10 }, revealDuration, function () {
                clearTimeout(builderAnimTimer[id][3]);
                builderAnimTimer[id][3] = setTimeout(function () {
                    hideWord($word);
                }, revealAnimationDelay);
            });
        }
    }

    function hideLetter($letter, $word, $bool, $duration) {
        $letter.removeClass('builder-aheading-in').addClass('builder-aheading-out');

        if (!$letter.is(':last-child')) {
            clearTimeout(builderAnimTimer[id][4]);
            builderAnimTimer[id][4] = setTimeout(function () {
                hideLetter($letter.next(), $word, $bool, $duration);
            }, $duration);
        } else if ($bool) {
            clearTimeout(builderAnimTimer[id][5]);
            builderAnimTimer[id][5] = setTimeout(function () {
                hideWord(takeNext($word));
            }, animationDelay);
        }

        if ($letter.is(':last-child') && builder_query('html').hasClass('builder-no-csstransitions')) {
            var nextWord = takeNext($word);
            switchWord($word, nextWord);
        }
    }

    function showLetter($letter, $word, $bool, $duration) {
        $letter.addClass('builder-aheading-in').removeClass('builder-aheading-out');

        if (!$word.hasClass('builder-is-visible')) {
            $word.parent().children().removeClass('builder-is-visible');
            $word.addClass('builder-is-visible');
        }

        if (!$letter.is(':last-child')) {
            clearTimeout(builderAnimTimer[id][6]);
            builderAnimTimer[id][6] = setTimeout(function () {
                showLetter($letter.next(), $word, $bool, $duration);
            }, $duration);
        } else {
            if (!$bool) {
                clearTimeout(builderAnimTimer[id][7]);
                builderAnimTimer[id][7] = setTimeout(function () {
                    hideWord($word);
                }, animationDelay)
            }
        }
    }

    function takeNext($word) {
        return (!$word.is(':last-child')) ? $word.next() : $word.parent().children().eq(0);
    }

    function switchWord($oldWord, $newWord) {
        $oldWord.removeClass('builder-is-visible').addClass('builder-is-hidden');
        if (!$newWord.hasClass('builder-is-visible')) {
            $newWord.removeClass('builder-is-hidden').addClass('builder-is-visible');
        }
    }
}

var builderSetInterval = {};
function builder_pl_row_slider(jEle) {
    var index = 0;

    var id = jEle.closest('[builder-id]').attr('builder-id');
    var imageEls = jEle.find('.builder-bgimg-slide'); // Get the images to be cycled.
    var speed = parseFloat(jEle.attr('data-speed')); // Get the speed of loop.
    imageEls.first().addClass('builder-slide-show');

    clearInterval(builderSetInterval[id]);
    builderSetInterval[id] = setInterval(function () {
        // Get the next index.  If at end, restart to the beginning.
        index = index + 1 < imageEls.length ? index + 1 : 0;

        // Show the next
        imageEls.eq(index).addClass('builder-slide-show');

        // Hide the previous
        imageEls.eq(index - 1).removeClass('builder-slide-show');
    }, speed);
}

