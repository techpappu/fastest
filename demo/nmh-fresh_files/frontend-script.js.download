//Grid Gallery Lightbox
function builder_pl_grid_lightbox(jEle) {

    // Grid Gallery pagination settings
    var gridCont = jEle.find('.builder-grid-gallery-container').children();
    var pagination = jEle.find('.builder-grid-gallery-pagination ul').children();
    gridCont.hide();
    gridCont.eq(0).show();
    // Adding event listners to pagination
    jEle.find('.builder-grid-page-item').each(function () {
        jQuery(this).on('click', function (event) {
            var text = jQuery(this).text();
            switch (text) {
                case '«':
                    builder_pl_grid_paginate(gridCont, pagination, 1, 0);
                    break;
                case '»':
                    builder_pl_grid_paginate(gridCont, pagination, (pagination.length - 2), (gridCont.length - 1));
                    break;
                default:
                    builder_pl_grid_paginate(gridCont, pagination, text, text - 1);
                    break;
            }
        });
    });

    // A tag will be there ONLY if the lightbox is on
    var a = jEle.find("[builder-grid-gallery-type=lightbox]");

    // No lightbox
    if (a.length < 1) {
        return;
    }

    a.nivoLightbox({
        effect: "fadeScale",
        keyboardNav: true,
        clickImgToClose: false,
        clickOverlayToClose: true,
    });
}

