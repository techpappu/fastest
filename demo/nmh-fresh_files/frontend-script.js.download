    jQuery('.builder-wp_menu').each(function () {
        builder_primary_menu(jQuery(this));
    });

    var new_vw = jQuery(document).width();

    if (new_vw == builder_doc_width) {
        return false;
    }

    builder_doc_width = new_vw;

    // Remove style
    jQuery('.builder-row-stretch-full').removeAttr('style');

    // Set a timeout to prevent bubbling
    setTimeout(function () {

        jQuery('.builder-row-stretch-full').each(function () {
            builder_pl_row_full(jQuery(this));
        });

    }, 200);

});






